{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.5"
        }
    },
    "routes": [
        {
            "src": "/(css|js|images|favicon.ico|build)/(.*)",
            "dest": "public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.php"
        }
    ]
}